<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="0dip"
                android:bottomRightRadius="10dip"
                android:topLeftRadius="0dip"
                android:topRightRadius="10dip"/>
            <stroke android:width="1dip" android:color="#6495ed"/>
            <gradient android:angle="-90" android:startColor="#6495ed" android:endColor="#6495ed"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="0dip"
                android:bottomRightRadius="10dip"
                android:topLeftRadius="0dip"
                android:topRightRadius="10dip"/>
            <stroke android:width="1dip" android:color="#ee3a6a"/>
            <solid android:color="#ee3a6a"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="0dip"
                android:bottomRightRadius="10dip"
                android:topLeftRadius="0dip"
                android:topRightRadius="10dip"/>
            <gradient android:angle="-90" android:startColor="#55FFFFFF" android:endColor="#55FFFFFF"/>
        </shape>
    </item>
    <item android:drawable="@drawable/baseline_search_black_24dp" />
</layer-list>